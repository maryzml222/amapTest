<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.example.gaodemapdemo.html.HtmlActivity">

    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="android 通过loadurl 调用js"
        android:onClick="btnLoadurlClick"/>
    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="android 通过evaluateJavascript 调用js"
        android:onClick="btnEvaluateJavascriptClick"/>

    <WebView
        android:id="@+id/webview"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>
</LinearLayout>
